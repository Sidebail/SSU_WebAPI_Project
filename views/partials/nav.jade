nav.navbar.navbar-expand-lg.navbar-light
  a.navbar-brand(href='/') Woodstock Online
  button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbar', aria-controls='navbar', aria-expanded='false', aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbar.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
    ul.navbar-nav
        li.nav-item
                a.btn.btn-github(href="/artist")
                    span.ml-2 Artists
        if isAuthenticated
            li.nav-item
            a.nav-link(href='/logout') Logout
        else
            li.nav-item
                a.btn.btn-github(href="/login")
                    span.ml-2 Login
        